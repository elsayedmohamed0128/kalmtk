  "author": "elsayed elisswi",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "backend": ".\\backend\\venv\\Scripts\\python.exe .\\backend\\app.py",
